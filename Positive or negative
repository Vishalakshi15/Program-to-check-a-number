import java.util.*;

public class Main
{
	public static void main(String[] args) 
	{   Scanner sc= new Scanner(System.in);
		System.out.println("N:");
		int N= sc.nextInt();
		if(N>0)
		System.out.print("Positive");
		else if(N>0)
		System.out.print("Negative");
		else
		System.out.print("Zero");
	}
}
